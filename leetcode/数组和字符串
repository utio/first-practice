//寻找数组中心索引，索引数左右数组元素和相同，没有返回-1
class Solution {
    public int pivotIndex(int[] nums) {
        int totle = 0; int index=0;
        int sum=0;
        for(int i=0; i<nums.length; i++){
            totle += nums[i];
        }
        for(int i=0; i<nums.length; i++){
            
            if(sum*2+nums[i] == totle){return i;}
            sum += nums[i];
        }
        return -1;
    }
}
